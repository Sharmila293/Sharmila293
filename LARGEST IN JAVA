//LARGEST IN JAVA
import java.util.Scanner;
class FirstName
{
	public static void main(String args[]) 
	{
           Scanner sc=new Scanner(System.in);
           Integer x,y,z;
           x=sc.nextInt();
           y=sc.nextInt();
           z=sc.nextInt();
if(x>y && x>z)
System.out.println(x);
else if(y>z && y>x)
System.out.println(y);
else
System.out.println(z);

}
}
